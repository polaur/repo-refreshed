#: kio_smb_auth.cpp:146
#, kde-format
msgid ""
"Please enter authentication information for:\n"
"Server = %1\n"
"Share = %2"
msgstr ""
"Lütfen aşağıdaki kimlik denetim bilgilerini girin:\n"
"Sunucu = %1\n"
"Paylaşım = %2"

#: kio_smb_auth.cpp:188
